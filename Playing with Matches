#include <stdio.h>
 
int main(void) {
	int t,a,b,s1,s2,r;
	scanf("%d",&t);
	while(t--)
	{
	    scanf("%d %d",&a,&b);
	    s1=a+b;
	    while(s1!=0)
	    {
	        r=s1%10;
	        if(r==0 || r==6 || r==9)
	        { 
	           s2=s2+6;
	        }
	        else if(r==1)
	        {
	         s2=s2+2;
	        }
	        else if(r==2 || r==3 || r==5)
	        {
	         s2=s2+5;
	        }
	        else if(r==4)
	        {
	         s2=s2+4;
	        }
	        else if(r==7)
	        {
	         s2=s2+3;
	        }
	        else if(r==8)
	        {
	         s2=s2+7;
	        }
	        s1=s1/10;
	    }
	    printf("%d\n",s2);
	    s2=0;
	}
	return 0;
}
