#include <stdio.h>

int main(void) 
{
	// your code goes here
	int t,i;
	scanf("%d",&t);
	for(i=0;i<t;i++)
	{
	     int n,k,count=0;
	     scanf("%d%d",&n,&k);
	     int a[n];
	     for(int j=0;j<n;j++)
	     {
	        scanf("%d",&a[j]);
	        if(a[j]>k)
	        {
	            count++;
	        }
	     }
	     printf("%d\n",count);
	} 
	return 0;
}
